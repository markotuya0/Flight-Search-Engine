# Flight Search Engine Environment Variables
# Copy this file to .env.local and fill in your values

# Amadeus API Configuration
# Get your credentials from: https://developers.amadeus.com/
VITE_AMADEUS_CLIENT_ID=your_amadeus_client_id_here
VITE_AMADEUS_CLIENT_SECRET=your_amadeus_client_secret_here

# Duffel API Configuration
# Get your credentials from: https://duffel.com/
DUFFEL_ACCESS_TOKEN=your_duffel_access_token_here

# API Configuration
VITE_API_BASE_URL=https://api.example.com
VITE_API_TIMEOUT=5000

# Feature Flags
VITE_ENABLE_ANALYTICS=false