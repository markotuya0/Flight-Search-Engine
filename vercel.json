{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/duffel/search.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "VITE_AMADEUS_CLIENT_ID": "@vite_amadeus_client_id",
    "VITE_AMADEUS_CLIENT_SECRET": "@vite_amadeus_client_secret"
  },
  "build": {
    "env": {
      "DUFFEL_ACCESS_TOKEN": "@duffel_access_token"
    }
  }
}